{
  "name": "esri-react-router-example",
  "version": "0.2.0",
  "description": "Example of how to use the ArcGIS API for JavaScript with react-router",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --content-base .",
    "build": "webpack && cp index.html dist/index.html",
    "deploy": "gh-pages-deploy",
    "clean-source": "rimraf README.md index.js modules package.json webpack.config.js"
  },
  "author": "Tom Wayson <tom@tomwayson.com> (https://tomwayson.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "esri-loader": "^0.1.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "gh-pages-deploy": "^0.4.2",
    "http-server": "^0.8.5",
    "rimraf": "^2.5.4",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "build"
    ],
    "noprompt": false
  }
}
